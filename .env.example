# DavinciMCP Environment Configuration
# Copy this file to .env and fill in your values

# ===== API KEYS =====
# Required for AI features - Get from https://ai.google.dev/
GEMINI_API_KEY=your_gemini_api_key_here

# ===== APPLICATION SETTINGS =====
# Log levels: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# ===== AI MODEL SETTINGS =====
# Controls randomness in AI responses (0.0-1.0)
GEMINI_TEMPERATURE=0.7
# Maximum tokens for AI responses
GEMINI_MAX_TOKENS=1024

# ===== FEATURE FLAGS =====
# Enable/disable operation feedback (true/false)
FEEDBACK_ENABLED=true

# ===== RESOLVE PATHS =====
# Custom path to Resolve modules (optional - auto-detected by default)
# RESOLVE_MODULES_PATH=/path/to/custom/resolve/modules 