# DaVinci Resolve Control - Sample Environment Variables
# Rename this file to .env and replace the sample values with your actual ones

# Google Gemini API key (Get one at https://makersuite.google.com/)
GEMINI_API_KEY=sample_gemini_api_key_replace_with_real_one

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Gemini AI Generation Parameters
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=1024

# Enable feedback messages (True/False)
FEEDBACK_ENABLED=True 